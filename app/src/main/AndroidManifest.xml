<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.scuber">

    <uses-permission android:name="android.permission.INTERNET" />


    <application
        android:usesCleartextTraffic="true"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name="com.example.scuber.login.MainLogin">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="com.example.scuber.ChargePoint" />
        <activity android:name="com.example.scuber.MainActivity" />
        <activity android:name="com.example.scuber.MyPage" />
        <activity android:name="com.example.scuber.giver.MainGiver" />
        <activity android:name="com.example.scuber.taker.MainTaker" />
        <activity android:name="com.example.scuber.taker.TakerRequests" />
        <activity android:name="com.example.scuber.giver.GiverRequests" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />

    </application>

</manifest>